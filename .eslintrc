{
	"env": {
		"browser": true,
		"es6": true,
		"commonjs": true,
		"node": true
	},

	"extends": ["airbnb", "prettier", "prettier/react"],
	"parserOptions": {
		"ecmaVersion": 9,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},

	"plugins": ["react", "prettier"],
	"parser": "babel-eslint",
	"rules": {
		"react/prop-types": 0,
		"react/jsx-uses-vars": [2],
		"no-console": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/destructuring-assignment":"off"


	}
}
